config_version=5

[application]

config/name="itoc"
run/main_scene="uid://5e2p7bsjwin2"
config/features=PackedStringArray("4.5", "C#", "Double Precision", "GL Compatibility")
run/max_fps=144
config/icon="res://icon.svg"

[autoload]

Core="*res://scripts/Core.cs"
GuiManager="*res://scripts/gui/GuiManager.cs"
CameraHelper="*res://scripts/camera/CameraHelper.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="itoc"

[gui]

theme/default_theme_scale=1.2
theme/default_font_multichannel_signed_distance_field=true

[importer_defaults]

